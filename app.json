{
  "expo": {
    "name": "Ledger",
    "slug": "ledger",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./ledger_logo.png",
    "splash": {
      "image": "./ledger_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nine4.ledger"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./ledger_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nine4.ledger"
    },
    "web": {
      "favicon": "./ledger_logo.png"
    },
    "scheme": "ledger",
    "newArchEnabled": false,
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-font",
      "@react-native-firebase/app"
    ]
  }
}
